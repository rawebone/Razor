{
    "name": "rawebone/razor",
    "description": "Razor is a small HTTP micro-framework",
    "tags": [ "http", "psr-7", "ioc", "micro", "framework", "small" ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Rawe",
            "email": "rawebone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Razor\\": [ "library/" ],
            "Razor\\Tests\\": [ "tests/" ]
        }
    },
    "require": {
        "php": ">=5.3",
        "psr/http-message": "0.*"
    },
    "require-dev": {
